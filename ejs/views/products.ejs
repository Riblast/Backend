<!DOCTYPE html>
<html lang="esp">

<body>
    <h1>Vista productos</h1>
    <% if (empty) { %>
        <h2>Lista de productos vac√≠a</h2>
        <% } else { %>
            <table>
                <thead>
                    <tr>
                        <td>Nombre</td>
                        <td>Precio</td>
                        <td>Foto</td>
                    </tr>
                </thead>
                <tbody>
                    <% products.forEach(product=> { %>
                        <tr>
                            <td>
                                <%= product.itemName %>
                            </td>
                            <td>
                                <%= product.itemPrice %>
                            </td>
                            <td><img src=<%=product.url %>></td>
                        </tr>
                        <% }) %>
                </tbody>
            </table>
            <% } %>
            <a href="/"><button>Agregar productos</button></a>
</body>

</html>